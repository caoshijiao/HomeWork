        if($('.topic-sidebar-box').length>0){
          $('.topic-sidebar-box').css('margin-top',(-$('.topic-sidebar-box').height()/2+'px'));
          $(function() {
            var app = {
              setScrollEvent: function() {
                $(window).on('scroll', function() {
                  var scrollTop;
                  if (navigator.userAgent.indexOf('Firefox') >= 0) {
                    scrollTop = document.documentElement.scrollTop;
                  } else if (navigator.userAgent.indexOf('IE') >= 0) {
                    scrollTop = document.documentElement.scrollTop;
                  } else {
                    scrollTop = $(document).scrollTop();    //$('body').scrollTop();
                  }
                  if (scrollTop > 600) {
                    $('.topic-sidebar-box').fadeIn();
                  } else {
                    $('.topic-sidebar-box').fadeOut();
                  }
                });
              },
              bindEvent: function() {
                $('.topic-sidebar-box').on('mouseenter', 'li.sidebar-item', function() {
                  $(this).find('.item-cur').css('opacity','1').siblings().hide();
                });
                $('.topic-sidebar-box').on('mouseleave', 'li.sidebar-item', function() {
                  $(this).find('.item-cur').css('opacity','0').siblings().show();
                });
              },
              init: function() {
                if($('.topic-sidebar-box').hasClass('autoHide')){$('.autoHide').hide();this.setScrollEvent();}
                this.bindEvent();
              }
            };
            app.init();
          });
          function mao(s,t){
            var p = -50;
            if(t){p = t;}
            $('html,body').stop().animate({scrollTop:$(s).offset().top+p},500);
          }
        }
        //解决banner图片浏览器缩小跑偏的问题
        var src = $('.banner img').attr('src');
        $('.banner').css('background','url('+src+') no-repeat top center');
        $.fn.unveil = function(threshold, callback) {
            var $w = $(window),
                th = threshold || 0,
                retina = window.devicePixelRatio > 1,
                attrib = retina? "data-src-retina" : "data-src",
                images = this,
                loaded;
            this.one("unveil", function() {
                var source = this.getAttribute(attrib);
                source = source || this.getAttribute("data-src");
                var srcImg = this.getAttribute('src');
                if(srcImg == source) return;
                if (source) {
                    this.setAttribute("src", source);
                    if (typeof callback === "function") callback.call(this);
                }
            });
            function unveil() {
                var inview = images.filter(function() {
                    var $e = $(this);
                    if ($e.is(":hidden")) return;

                    var wt = $w.scrollTop(),
                        wb = wt + $w.height(),
                        et = $e.offset().top,
                        eb = et + $e.height();

                    return eb >= wt - th && et <= wb + th;
                });
                loaded = inview.trigger("unveil");
                images = images.not(loaded);
            }
            $w.on("scroll.unveil resize.unveil lookup.unveil mousemove.unveil", unveil);
            unveil();
            return this;
        };
        $('img').unveil();
        $('.pop-block').on('click',function() {
            $(this).parent().find('.pop-mask').removeClass('hide');
        });
        $('.pop-close').on('click',function(){
            $('.pop-mask').addClass('hide');
        });

        //弹幕

        function Barrage(configData) {
            this.url = {
                getBarrageListAjax: 'https://api.tuniu.com/topic/topicCollection'
            };
            this.config = {
                defaultAvatar: 'http://m.tuniucdn.com/fb2/t1/G1/M00/78/90/Cii9EVidLHOIflFrAAARPEd3gL8AAHaVgNy3rgAABFU217_w60_h60_c1_t0.png',
                fColor: configData.fColor || '#383838',
                fSize: parseInt(configData.fSize) || 14,
                bgColor: configData.bgColor || '#fff',
                bgX: 0,
                bgY: 0,
                bgWidth: 0,
                bgHeight: 0
            }
            id = configData.id || '';
            this.wrapperdom= $('#barrageWrapper' + id);
            this.pipevalue = [];//每个通道中弹幕值集合[[],[],[]]
            this.pipenum = 5;//通道数

            var position = configData.bgLocate || '0*0';
            position = position.split('*');
            this.config.bgX = parseInt(position[0]);
            this.config.bgY = parseInt(position[1]);

            var size = configData.bgSize || '0*0';
            size = size.split('*');
            this.config.bgWidth = parseInt(size[0]);
            this.config.bgHeight = parseInt(size[1]);
        }
        Barrage.prototype = {
            constructor: Barrage,
            //获取弹幕
            getBarrageList: function(tid, type) {
                var self = this;
                $.ajax({
                    url: self.url.getBarrageListAjax,
                    dataType: 'json',
                    method: 'GET',
                    data:{
                        d: JSON.stringify({
                            topicId: tid,
                            page: 1,
                            limit: 100,
                            tabKey: type || 'all',
                        }),
                        c: JSON.stringify({
                            ct: 100
                        })
                    },
                    success: function(json) {
                        var barragePool = json.data.cardList.map(function (obj) {
                            return {imgUrl:obj.userInfo.userHeadImg,content:obj.contentInfo.title};
                        });
                        self.scoop(barragePool);
                    }
                });
            },
            //弹幕发送
            transmit: function(pipevals, pipeline, speedval, basetimer) {
                var self = this,
                savearry = JSON.stringify(pipevals),
                baseTime = basetimer || 15000 + Math.floor(Math.random() * 2000),
                speed = speedval || 0,
                delay = speedval ? 0 : Math.floor(Math.random() * 3000),
                wHeight = self.config.bgHeight; //画布高
                wWidth =  self.config.bgWidth; // 画布宽

                pipevals.forEach(function (value, index){
                    var content =  value.content,
                    imgUrl = value.imgUrl || self.config.defaultAvatar;
                    if (content.length > 30) {
                        var num = 30;
                        content = content.substr(0, num) + '...';
                    }

                    var div = $('<div class="single clearfix"><p><img src="' + imgUrl +'"/>' + content + '</p></div>');
                    self.wrapperdom.append(div);
                    div.css({
                        'line-height': self.config.fSize + 'px',
                    });
                    var domWidth = div.width(), //当前单条弹幕宽
                    domHeight = div.height(), //当前单条弹幕高
                    pipeHeight = wHeight / self.pipenum, // 单条通道高
                    domTop = (pipeHeight * pipeline) + Math.floor((pipeHeight - domHeight) * Math.random());
                    if (index == 0 && !speedval) {
                        speed = baseTime / (wWidth + domWidth);
                        timer = baseTime;
                    } else {
                        var timer = speed * (wWidth + domWidth);
                    }

                    div.css({
                        'background': self.config.bgColor,
                        'animation-name': 'barrageAnima',
                        'animation-timing-function': 'linear',
                        'animation-duration': timer + 'ms',
                        'animation-delay': delay + 'ms',
                        'top': domTop + 'px'
                    });
                    delay +=  speed * (wWidth / 5 + domWidth);//当前一条弹幕结束right到达屏幕的1/5处时，下一条弹幕进入
                    div.on('animationend',function (e) {
                        div.remove();
                    });
                    if (index == pipevals.length - 1) {
                        var setTime = setTimeout(function() {
                            savearry = JSON.parse(savearry);
                            self.transmit(savearry, pipeline, speed, baseTime);
                            clearTimeout(setTime);
                        }, delay);
                    }
                });
            },
            init: function(values, type) {
                var self = this;

                //弹幕画布初始化,在banner插入弹幕展示区域
                var maxWidth = $('.banner').width(),//banner宽
                maxHeight = $('.banner').height(); //banner高
                this.wrapperdom = $('<div id="barrageWrapper' + id + '" class="barrage">\
                    <div class="single"><p><img src="' + self.config.defaultAvatar + '" alt="">\
                    #这是条弹幕#一大波弹幕来袭~~</p></div></div>');
                $('.banner').append(this.wrapperdom);

                if (self.config.bgWidth > maxWidth || self.config.bgWidth == 0) {
                    self.config.bgWidth = maxWidth;
                }
                if (self.config.bgHeight > maxHeight || self.config.bgHeight == 0) {
                    self.config.bgHeight = maxHeight;
                }
                if (self.config.bgX > maxWidth || self.config.bgY > maxHeight) {
                    self.wrapperdom.remove();
                    return;
                };

                // 弹幕动画前后不改变left属性值，避免元素reflow，js中获取弹幕节点所在的父元素的宽度，根据宽度来定义弹幕节点的初始位置。
                var style = document.createElement('style');
                document.head.appendChild(style);
                var from = 'from { visibility: visible; -webkit-transform: translate3d(' + maxWidth + 'px,0,0); }';
                var to = 'to { visibility: visible; -webkit-transform: translate3d(-100%,0,0); }';
                style.sheet.insertRule('@-webkit-keyframes barrageAnima { ' + from + to + '}', 0);

                self.wrapperdom.css({
                    'font-size': self.config.fSize + 'px',
                    'color': self.config.fColor,
                    'left': self.config.bgX + 'px',
                    'top': self.config.bgY + 'px',
                    'width': self.config.bgWidth + 'px',
                    'height': self.config.bgHeight + 'px'
                });
                // 根据画布大小设置通道数
                $('.single').css({
                    'font-size': self.config.fSize + 'px',
                    'line-height': self.config.fSize + 'px',
                });
                var pipeMinHeight = $('.single').height();

                var pipeRate = this.config.bgHeight/pipeMinHeight;
                if (pipeRate > 6) {
                    self.pipenum = 5;
                } else if (pipeRate < 1) {
                    pipenum = 0;
                    this.wrapperdom.remove();
                } else {
                    self.pipenum = Math.floor(pipeRate);
                }

                // 判断传入是弹幕池或话题ID
                if ((typeof values == 'number' || typeof values == 'string') && type) {
                    self.getBarrageList(values, type);
                } else if (typeof values == 'object') {
                    self.scoop(values);
                }
            },
            //从弹幕池捞取弹幕
            scoop: function(allValues) {
                var self = this;
                var arr = allValues;
                if (arr.length < self.pipenum) {
                    self.pipenum = arr.length;
                }
                self.pipevalue = Object.keys(Array.from({ length: self.pipenum })).map(function() {
                    return [];
                });
                var arraylength = Math.floor(allValues.length / self.pipenum);

                for (i = 0; i < self.pipenum; i++) {
                    if (i < self.pipenum -1) {
                        var now = 0;
                        arr.forEach(function(item) {
                            if (now >= arraylength) return;
                            var rnum = Math.floor(Math.random() * (arr.length));
                            self.pipevalue[i].push(arr[rnum]);
                            arr.splice(rnum,1);
                            now++;
                        });
                    } else {
                        self.pipevalue[i] = arr;
                    }
                };
                self.pipevalue.forEach(function(pipevals, tube) {
                    self.transmit(pipevals, tube);
                });
            }
        }

        //发送弹幕组件
        var SendBarrge = {
            config: {
                defaultAvatar: 'http://m.tuniucdn.com/fb2/t1/G1/M00/78/90/Cii9EVidLHOIflFrAAARPEd3gL8AAHaVgNy3rgAABFU217_w60_h60_c1_t0.png',
                ifClick: false
            },
            //获取发送弹幕配置
            getSendBarrageConfig: function(initData) {
                var self = this;
                var wrapperdom = $('.send-barrage-module');
                $('body').css({'padding-bottom': '100px'});

                wrapperdom.css({'background-image':'url(' + initData.bgurl + ')'});
                var div = '<div class="send-barrage-wrapper">\
                                <div class="send-barrage clearfix">\
                                    <input type="text"  maxlength="100"  name="barrageContent" placeholder="'+ initData.tip +'">\
                                    <div class="button" style="background-image:url('+ initData.btn +')"></div>\
                                </div>\
                            </div>';
                wrapperdom.append(div);
                wrapperdom.find('.button').on('click', function() {
                    var content = wrapperdom.find('input').val();

                    self.config.topicId = initData.t_id;
                    self.config.contentPre = initData.pre || '';
                    self.config.contentAfter = initData.after || '';
                    self.config.contentPic = initData.img || '';

                    self.addContent(content);
                });

            },
            addContent: function(content) {
                var self = this;
                if (!content) return;

                var self = this;
                var timestamp = new Date().getTime();

                var data = {
                    type: 1001,
                    content: {
                        title:'',
                        recentTimestamp: timestamp,
                        agreementChecked: true,
                        shareType: 0,
                        coverImageUrl: '',
                        elements: [
                            {
                                elementType: 0,
                                elementId: 0,
                                content: self.config.contentPre + content + self.config.contentAfter
                            }
                        ]
                    },
                    topicId: self.config.topicId
                };
                if (self.config.contentPic) {
                    var img = new Image();
                    img.src = self.config.contentPic;
                    img.onload = function() {
                        var imgWidth = img.width;
                        var imgHeight = img.height;
                        data.content.elements.push({
                            elementType:1,
                            imgUrl: self.config.contentPic,
                            elementId: 1,
                            imgWidth: imgWidth,
                            imgHeight:imgHeight,
                            pcUrl: '',
                            appUrl:'',
                            mUrl:'',
                            imgDescription:'',
                            poi: {poiId: 0, poiName:'', poiType: 0, parentId:0,parentName:'',imgUrl:'',typeName:0}
                        });
                        self.addContentAjax(data,content);
                    };
                } else {
                    self.addContentAjax(data,content);
                }
            },
            addContentAjax: function(data,content) {
                var self = this;
                if (self.config.ifClick) return;
                self.config.ifClick = true;

                data.content = JSON.stringify(data.content);
                $.ajax({
                    type: 'get',
                    url: '//m.tuniu.com/event/innerTemporaryActivity/saveContentV2Ajax?isJsonp=1',
                    dataType: 'jsonp',
                    jsonp:'callback',
                    data: data,
                    success: function(json){
                        self.config.ifClick = false;
                        if(json.success && json.data && json.data.contentId) {
                            $('.send-barrage-module').find('input').val('');
                            if (bConfig.tid && $('.barrage').length) {
                                var fsize =  bConfig.fsize || 14;
                                if (content.length > 30) {
                                    content = content.substr(0, 30) + '...';
                                }
                                var avatar = json.data.headImg || self.config.defaultAvatar;
                                var div = $('<div class="single clearfix"><p><img src="' + avatar +'"/>' + content + '</p></div>');
                                $('.barrage').append(div);
                                var wrapperHeight = $('.barrage').height();
                                if (!wrapperHeight) return;
                                domHeight = div.height();
                                var top = Math.floor((wrapperHeight - domHeight) * Math.random());
                                div.css({
                                    'background': '#fff',
                                    'animation-name': 'singleBarrage',
                                    'animation-timing-function': 'linear',
                                    'animation-duration': 10000 + 'ms',
                                    'animation-delay': 0 + 'ms',
                                    'top': top + 'px',
                                    'font-size': fsize + 'px',
                                    'line-height': fsize + 'px',
                                    'border':'1px solid #f80'
                                });
                                div.on('animationend',function (e) {
                                    div.remove();
                                });
                            }
                        }else {
                            if (json.data.loginStatus == false) {
                                window.location.href = '/u/login?urlRefer=' + window.btoa(window.encodeURIComponent(window.location.href));
                            }
                        }
                    },
                    error: function(err){
                        self.config.ifClick = false;
                        console.log('请求异常');
                    }
                });
            }
        }

        var $barrageDom = $('#bConfig');
        var bConfig = {};
        var $sendBarDom = $('#sendConfig');
        var sendConfig = {};
        window.onload = function() {
            if ($barrageDom && $barrageDom.length > 0) {
                bConfig = $barrageDom.val();
                bConfig = JSON.parse(bConfig);
            }

            var barrage_id = bConfig.tid || '';
            if (barrage_id) {
                var barrageConfig = {};
                barrageConfig.id =  bConfig.mid || '';
                barrageConfig.fColor =  bConfig.fcolor || '';
                barrageConfig.fSize = bConfig.fsize || '';
                barrageConfig.bgSize =  bConfig.bgsize || '';
                barrageConfig.bgLocate =  bConfig.bglocate || '';
                barrageConfig.bgColor =  bConfig.bgcolor || '';
                var essence_flag = bConfig.excellent || '';
                var type = essence_flag  && essence_flag == 1 ? 'excellent' : 'all';
                var barrage = new Barrage(barrageConfig);

                var bannerimg = $('.banner img');
                var src = bannerimg.attr('src');
                bannerimg.attr('src', '');
                if(bannerimg.length && bannerimg.length > 0) {
                    bannerimg.on('load',function() {
                        console.log('success');
                        barrage.init(barrage_id, type);
                    });
                }
                bannerimg.attr('src', src);
            }

            if ($sendBarDom.length && $sendBarDom.length > 0) {
                sendConfig = $sendBarDom.val();
                sendConfig = JSON.parse(sendConfig);
                SendBarrge.getSendBarrageConfig(sendConfig);
            }
        }

        //抽奖
        var Lottery={
            url:{
                lotteryAndSendAjax:'https://m.tuniu.com/event/lottery/opeLottery/lotteryAndSendAjax',
                getLoginInfoAjax:'https://m.tuniu.com/event/geLottery/getLoginInfoAjax',
                getTelCodeAjax:'https://m.tuniu.com/event/raffle/getTelCodeAjax',
                getStyleInfoAjax: 'https://m.tuniu.com/event/innerLink/getStyleInfoAjax'
            },
            init:function(){
                var self=this;
                self.initListener();
                if (parseInt($('#linkId').val())) {
                    $.ajax({
                        url: self.url.getStyleInfoAjax,
                        dataType: 'jsonp',
                        data: {
                            id: parseInt($('#linkId').val())
                        },
                        success: function(json) {
                            if (json && json.success && json.data && json.data.pc) {
                                var style = json.data.pc.style,
                                    styleParse = JSON.parse(window.decodeURIComponent(atob(style)));
                                $('body').append(styleParse.demo);
                                $('head').append('<style>' + styleParse.css + '</style>');
                            }
                        }
                    });
                }
            },
            initListener:function(){
                var self=this;
                $('.lottery-pic').on('click',function(){
                    var dom=$(this).parent();
                    var data={
                        actId:dom.find('.lottery-mask').attr('lottery-id'),
                        mark:dom.find('.lottery-mask').attr('lottery-mark'),
                        tel:dom.find('.lottery-mobile').val(),
                        checkCode:dom.find('.lottery-code').val(),
                        sysType:1,
                        isJsonp:1,
                        type:1,
                        one:1,
                    };
                    $.ajax({
                        url: self.url.lotteryAndSendAjax,
                        dataType: 'jsonp',
                        data:data,
                        success: function(json) {
                            self.lottery(json,dom);
                        }
                    });
                });
                $('.lottery-submit').on('click',function(){
                    var dom=$(this).parents('.lottery-mask');
                    var data={
                        actId:dom.attr('lottery-id'),
                        mark:dom.attr('lottery-mark'),
                        tel:dom.find('.lottery-tel').val(),
                        checkCode:dom.find('.lottery-check-code').val(),
                        sysType:1,
                        isJsonp:1,
                        type:1,
                        one:1,
                    };
                    $.ajax({
                        url: self.url.lotteryAndSendAjax,
                        dataType: 'jsonp',
                        data:data,
                        success: function(json) {
                            self.lottery(json,dom);
                        }
                    });
                });
                $('.lottery-get-check').on('click',function(){
                    var dom=$(this).parents('.lottery-mask');
                    if(!$(this).hasClass('sending')){
                        var data={
                            actId:dom.attr('lottery-id'),
                            mark:dom.attr('lottery-mark'),
                            tel:dom.find('.lottery-tel').val(),
                            isJsonp: 1
                        }
                        $.ajax({
                            url: self.url.getTelCodeAjax,
                            dataType: 'jsonp',
                            method: 'GET',
                            data:data,
                            success: function(json) {
                                if(json.errorCode==710000){
                                    dom.find('.lottery-get-check').addClass('sending');
                                    self.checkCodeCountdown(60,dom);
                                }else if(json.errorCode==710002){
                                    self.chokeWarn('您每天最多只能获取三次验证码');
                                }else if(json.errorCode==710004){
                                    self.chokeWarn('请输入有效的11位手机号');
                                }else{
                                    self.chokeWarn('发生错误，请稍后再试');
                                }
                            }
                        });
                    }
                });
                $('.lottery-get-code').on('click',function(){
                    var dom=$(this).parent();
                    if(!$(this).hasClass('sending')){
                        var data={
                            actId:dom.find('.lottery-mask').attr('lottery-id'),
                            mark:dom.find('.lottery-mask').attr('lottery-mark'),
                            tel:dom.find('.lottery-mobile').val(),
                            isJsonp: 1
                        }
                        $.ajax({
                            url: self.url.getTelCodeAjax,
                            dataType: 'jsonp',
                            method: 'GET',
                            data:data,
                            success: function(json) {
                                if(json.errorCode==710000){
                                    dom.find('.lottery-get-code').addClass('sending').css('background','url('+dom.find('.lottery-get-code').attr('active')+') no-repeat');
                                    self.checkCodeCountdown(60,dom);
                                }else if(json.errorCode==710002){
                                    self.chokeWarn('您每天最多只能获取三次验证码');
                                }else if(json.errorCode==710004){
                                    self.chokeWarn('请输入有效的11位手机号');
                                }else{
                                    self.chokeWarn('发生错误，请稍后再试');
                                }
                            }
                        });
                    }
                });
                $('.lottery-close,.lottery-use').on('click',function(){
                    $('.lottery-mask,.lottery-list,.lottery-error,.lottery-apply').addClass('hide');
                });
                $('.lottery-block').on('click',function(){
                    var dom=$(this).parent();
                    var data={
                        actId:dom.find('.lottery-mask').attr('lottery-id'),
                        mark:dom.find('.lottery-mask').attr('lottery-mark'),
                        sysType:1,
                        isJsonp:1,
                        type:0,
                        one:1,
                    };
                    $.ajax({
                        url: self.url.lotteryAndSendAjax,
                        dataType: 'jsonp',
                        data:data,
                        success: function(json) {
                            if(json.errorCode==710002){
                                dom.find('.lottery-mask,.lottery-apply').removeClass('hide');
                            }else{
                                self.lottery(json,dom);
                            }
                        }
                    });
                });
            },
            lottery:function(json,dom){
                var self=this;
                if(json.errorCode==710000 && json.data.status==1){
                    var list=json.data.prizeInfo.package,html='';

                    for(var i=0;i<list.length;i++){
                        html+='<div class="lottery-item">\
                            <div class="lottery-price-name">¥<span>'+list[i].amount+'</span></div>\
                            <div class="lottery-detail">'+list[i].name+'</div>\
                        </div>';
                    }
                    dom.find('.lottery-content').html(html);
                    dom.find('.lottery-title').html('领取成功!');
                    dom.find('.lottery-mask,.lottery-list').removeClass('hide');
                    if(list.length==1){
                        $('.lottery-item').css('margin-top','60px');
                    }
                    $('.lottery-apply').addClass('hide');
                }else if(json.errorCode==710008){
                    var list=json.data.prizeInfo.package,html='';
                    for(var i=0;i<list.length;i++){
                        html+='<div class="lottery-item">\
                            <div class="lottery-price-name">¥<span>'+list[i].amount+'</span></div>\
                            <div class="lottery-detail">'+list[i].name+'</div>\
                        </div>';
                    }
                    dom.find('.lottery-content').html(html);
                    dom.find('.lottery-title').html('已领取');
                    dom.find('.lottery-mask,.lottery-list').removeClass('hide');
                    if(list.length==1){
                        $('.lottery-item').css('margin-top','60px');
                    }
                    $('.lottery-apply').addClass('hide');
                }else if(json.errorCode==710001){
                    dom.find('.lottery-title').html('领取失败');
                    dom.find('.lottery-error').find('span').html('参数错误喽~');
                    dom.find('.lottery-mask,.lottery-error').removeClass('hide');
                    $('.lottery-apply').addClass('hide');
                }else if(json.errorCode==710002){
                    dom.find('.lottery-title').html('领取失败');
                    dom.find('.lottery-error').find('span').html('要先登录哦~');
                    dom.find('.lottery-mask,.lottery-error').removeClass('hide');
                    $('.lottery-apply').addClass('hide');
                }else if(json.errorCode==710003){
                    dom.find('.lottery-title').html('领取失败');
                    dom.find('.lottery-error').find('span').html('活动结束啦~');
                    dom.find('.lottery-mask,.lottery-error').removeClass('hide');
                    $('.lottery-apply').addClass('hide');
                }else if(json.errorCode==710004){
                    dom.find('.lottery-title').html('领取失败');
                    dom.find('.lottery-error').find('span').html('抽奖机会用完啦~');
                    dom.find('.lottery-mask,.lottery-error').removeClass('hide');
                    $('.lottery-apply').addClass('hide');
                }else if(json.errorCode==710005){
                    self.chokeWarn('请输入正确的验证码');
                }else if(json.errorCode==710006){
                    self.chokeWarn('验证码已过期');
                }else if(json.errorCode==710007){
                    self.chokeWarn('请输入正确的手机号');
                }else if(json.errorCode==710012){
                    dom.find('.lottery-title').html('领取失败');
                    dom.find('.lottery-error').find('span').html('新用户才能领取哦~');
                    dom.find('.lottery-mask,.lottery-error').removeClass('hide');
                    $('.lottery-apply').addClass('hide');
                }else if(json.errorCode==710013){
                    dom.find('.lottery-title').html('领取失败');
                    dom.find('.lottery-error').find('span').html('注册用户才能领取哦~');
                    dom.find('.lottery-mask,.lottery-error').removeClass('hide');
                    $('.lottery-apply').addClass('hide');
                }else if(json.errorCode==710014){
                    dom.find('.lottery-title').html('领取失败');
                    dom.find('.lottery-error').find('span').html('星级会员才能领取哦~');
                    dom.find('.lottery-mask,.lottery-error').removeClass('hide');
                    $('.lottery-apply').addClass('hide');
                }else if(json.errorCode==710015){
                    dom.find('.lottery-title').html('领取失败');
                    dom.find('.lottery-error').find('span').html('非星级会员才能领取哦~');
                    dom.find('.lottery-mask,.lottery-error').removeClass('hide');
                    $('.lottery-apply').addClass('hide');
                }else if(json.errorCode==710009){
                    dom.find('.lottery-title').html('领取失败');
                    dom.find('.lottery-error').find('span').html('操作失败了哦~');
                    dom.find('.lottery-mask,.lottery-error').removeClass('hide');
                    $('.lottery-apply').addClass('hide');
                }else if(json.errorCode==710011){
                    dom.find('.lottery-title').html('领取失败');
                    dom.find('.lottery-error').find('span').html('活动还未开始哦~');
                    dom.find('.lottery-mask,.lottery-error').removeClass('hide');
                    $('.lottery-apply').addClass('hide');
                }else if(json.errorCode==710099){
                    dom.find('.lottery-title').html('领取失败');
                    dom.find('.lottery-error').find('span').html('网络异常啦~');
                    dom.find('.lottery-mask,.lottery-error').removeClass('hide');
                    $('.lottery-apply').addClass('hide');
                }else if(json.errorCode==710016){
                    dom.find('.lottery-title').html('领取失败');
                    dom.find('.lottery-error').find('span').html('抱歉，未中奖哦~');
                    dom.find('.lottery-mask,.lottery-error').removeClass('hide');
                    $('.lottery-apply').addClass('hide');
                }else if(json.errorCode==710025){
                    dom.find('.lottery-title').html('领取失败');
                    dom.find('.lottery-error').find('span').html('请勿频繁操作~');
                    dom.find('.lottery-mask,.lottery-error').removeClass('hide');
                    $('.lottery-apply').addClass('hide');
                }else{
                    dom.find('.lottery-title').html('领取失败');
                    dom.find('.lottery-error').find('span').html('发生错误，请稍后再试');
                    dom.find('.lottery-mask,.lottery-error').removeClass('hide');
                    $('.lottery-apply').addClass('hide');
                }
            },
            checkCodeCountdown:function(t,dom){
                var self=this;
                if(t>0){
                    t--;
                    dom.find('.lottery-get-code,.lottery-get-check').html(t+'s');
                    setTimeout(function(){
                        self.checkCodeCountdown(t,dom);
                    },1000);
                }else{
                    dom.find('.lottery-get-code,.lottery-get-check').html('');
                    dom.find('.lottery-get-check').removeClass('sending');
                    dom.find('.lottery-get-code').removeClass('sending').css('background','url('+dom.find('.lottery-get-code').attr('normal')+') no-repeat');
                }
            },
            chokeWarn:function(str){
                $('.code-warn').html(str).css('display','block').addClass('bounceIn');
                setTimeout(function(){
                    $('.code-warn').css('display','none').removeClass('bounceIn');
                },2000);
            },
        };
        Lottery.init();
        $('.slider').each(function(){
            var id=$(this).attr('id'),
                dir=$(this).attr('dir'),
                active=0,
                as=$('.pagenavi-'+id).find('a');
            var ts=new TouchSlider({
                id: id,
                speed: 600,
                timeout: 3000,
                direction:dir,
                before: function(index) {
                    as.get(active).className = '';
                    active = index;
                    as.get(active).className = 'active';
                }
            });
            as.on('click',function(){
                ts.slide($(this).index(),600);
            })
        });
    </script>
