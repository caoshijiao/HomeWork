      window.PageMessage.wait('tvp-ready', boot);
    setTimeout(boot, 5000);

    var booted = false;
    function boot() {
        if (booted) return;
        booted = true;
        $.getScript('//vm.gtimg.cn/tencentvideo/script/vplay2017/2004021623/vplay.lib.js', function () {
            
            $.getScript('//vm.gtimg.cn/tencentvideo/script/vplay2017/2004021623/vplay.page.js')
            
        })
    }
</script>

</div>

</body>
</html>
