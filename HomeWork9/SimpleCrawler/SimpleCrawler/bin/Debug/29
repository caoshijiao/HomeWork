<script type="text/javascript" src="//vm.gtimg.cn/tencentvideo_v1/script/txv.core.js?max_age=604800&v=20190402&_cross=1" crossorigin="anonymous" r-notemplate="true"></script>
<script type="text/javascript">
    $.getScript = function (url, cb) {
        // 从非跨域加载切换到跨域加载，需要加个版本号，防止因缓存导致的问题
        if (typeof url != 'string') {
            return ;
        } 
        url = url.indexOf('?') != -1 ? url + "&_cross=1" : url + "?_cross=1";
        return $.ajax({
            crossDomain: true,
            type: "GET",
            url: url,
            dataType: "script", 
            success: function (){
                typeof cb == 'function' && cb();
            },
            error: function (e) {
                typeof cb == 'function' && cb(e)
            }
        });
    }
</script>

<script type="text/javascript">window.player_time_speed.playerjs=(new Date).getTime();window.player_time_speed.js=(new Date()).getTime();window.jsVersion = {flash:'20171208'}</script>
<script type="text/javascript" src="//vm.gtimg.cn/tencentvideo/txpv5/js/txplayer.js?max_age=86400&v=20200401&_cross=1" crossorigin="anonymous"></script>
<script type="text/javascript">window.player_time_speed.js=(new Date()).getTime() - window.player_time_speed.js;</script>

