   e=window.event||e;
   var host=window.location.host;
   var hosts=host.replace(/\/.*?([^\.]+\.(com\.cn|org\.cn|net\.cn|[^\.]+))\/.+/, "$1");
     switch(e.keyCode){
     case 37: //左键
          if($("#showkey").attr("data-hasfu")!="1"){
            if(n>1){
              n-=1;
              var url=$(".page").find('.page'+n+'').attr("href");
              if(url!=undefined){
                window.location.href='https://bbs.hupu.com/'+url+'';
            }
          }
        }
       break;
     case 39: //右键
          if($("#showkey").attr("data-hasfu")!="1"){
            if(n<9){
              n+=1;
              var url=$(".page").find('.page'+n+'').attr("href");
              if(url!=undefined){
                window.location.href='https://bbs.hupu.com/'+url+'';
            }
          }
        }
       break;
     default:
       break;
   }

  }
