<script type="text/javascript">
  (function($){
    var oSideNavLi= $('.sideNav ul li');
      var oSideBack = $('.sideNav .back');
      if(oSideNavLi.length <= 0){
        oSideBack.hide();
      }else{
        oSideBack.show();
      }

      var num = $('.block_mod').length - 1;
      var aTop = [];
      $('.block_mod').each(function() {
          var sTop = $(this).offset().top - 200;
          aTop.push(sTop);
      });
      
      $(window).scroll(function(event) {
          var aW = parseInt($(window).scrollTop());
          var aIndex = 0;
          if ($(window).scrollTop() > 300) {
              $('.sideNav').show();
          } else {
              $('.sideNav').hide();
          }
          for (var i = 0; i <= num; i++) {
              if (aW >= aTop[i] && aW < aTop[i + 1]) {
                  aIndex = i;
                  break;
              }
              if (i == num && aW >= aTop[i]) {
                  aIndex = i;
                  break;
              }
          }
          $('.sideNav li').eq(aIndex).addClass('active').siblings().removeClass('active');
      });
      $(".sideNav li").click(function(event) {
          $("body,html").stop().animate({
              "scrollTop": $(".block_mod").eq($(this).index()).offset().top
          },
          1000);
      });
      $('.back').click(function() {
          $("body,html").stop().animate({
              "scrollTop": 0
          },
          800);
      })
  })(jQuery)
</script>
