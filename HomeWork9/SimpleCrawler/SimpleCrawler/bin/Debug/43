    $("#size-list dd").click(function () {
        $("#size-list dd").removeClass('null').removeClass("checked");
        $(this).addClass("checked");
        var sizeId = $(this).attr("data-id");
        $("#color-list dd").removeClass('have');
        // $("#color-list dd").removeClass('checked');
        $("#color-list dd").addClass('null');
         $.each(skuinfo, function(key, val){
            if(val.size == sizeId){
                // console.log(val.color);
                $("#color-list dd[data-id='"+val.color+"']").removeClass('null').addClass("have");
            }
        });
    });

    $("#color-list dd").click(function () {
        $("#color-list dd").removeClass('null').removeClass("checked");
        $(this).addClass("checked");
        var colorId = $(this).attr("data-id");
        $("#size-list dd").removeClass('have');
        // $("#size-list dd").removeClass('checked');
        $("#size-list dd").addClass('null');
        $.each(skuinfo, function(key, val){
            if(val.color == colorId){
                // console.log(val.size);
                $("#size-list dd[data-id='"+val.size+"']").removeClass('null').addClass("have");
            }
        });
    });

