      var perfix = location.host.substring(0, location.host.indexOf("."));
      if (perfix == "courseweb" || perfix == "mooc") {
        window.location = "publicCourse/index.html#/index/portal";
      } else if (perfix == "www") {
        if (location.host == "www.ulearning.app") {
          window.location = "ulearning/index.html#/i18n";
        } else if (location.host == "www.ulearning.asia") {
          window.location = "ulearning/index.html#/i18n";
        } else {
          window.location = "ulearning/index.html#/index/portal";
        }
      } else if (perfix == "rmsc") {
        // 人民双创
        window.location = "sc/index.html#/index/portal";
      } else if (perfix == "hnszlm") {
        // 思政课程联盟
        window.location = "hnszCourseLeague/index.html#/index/portal";
      } else if (perfix == "x") {
        window.location = "screen/index.html";
      } else if (perfix == "js") {
        window.location = "militaryTheory/index.html#/index/portal";
      } else if (perfix == "tjxc") {
        // 天津财大门户
        window.location = "portal-site/index.html#/tjxc";
      } else if (perfix == "right") {
        // 权限管理系统
        window.location = "right-ms/manage/index.html#/login";
      } else if (perfix == "hjy") {
        // 天津海教
        window.location = "portal-site/index.html#/tjhj";
      } else if (perfix == "xdmy") {
        // 天津现代
        window.location = "portal-site/index.html#/tjxd";
      } else {
        window.location = "organization/index.html#/index/portal";
      }
    </script>
