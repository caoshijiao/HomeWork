    <script type="text/javascript">
        function getAnnouncement() {
            $.ajax({
                //几个参数需要注意一下
                type: "GET",//方法类型
                //dataType: "json",//预期服务器返回的数据类型
                url: "/announcement/indexList",//url
                data: {},
                success: function (result) {
                    $("#demo1").empty();
                    $("#demo2").empty();
                    var dataLength = result.data.length;
                    var html = "<ul class='mb0'><li>&nbsp</li>";
                    if (dataLength != 0) {
                        for (var i = 0; i < dataLength; i++) {
                            var item = result.data[i];
                            html = html + "<li><a target='_blank' href='/announcement/view?id=" + item.id + "' style='color: #aa1e11'>" + item.title + "</a></li>";
                        }
                    } else {
                        html = html + "<li><a style='color: #aa1e11'>" + "暂无公告" + "</a></li>";
                    }
                    html = html + "</ul>";

                    $("#demo1").html(html);
                    $("#demo2").html(html);
                },
                error: function (error) {
                    $('#announcementList').html("暂无公告");
                }
            });
        };

        var demo = document.getElementById("demo");
        var demo1 = document.getElementById("demo1");
        var demo2 = document.getElementById("demo2");
        demo2.innerHTML = document.getElementById("demo1").innerHTML;

        function Marquee() {
            if (demo.scrollLeft - demo2.offsetWidth >= 0) {
                demo.scrollLeft -= demo1.offsetWidth;
            } else {
                demo.scrollLeft++;
            }
        }

        var myvar = setInterval(Marquee, 30);
        demo.onmouseout = function () {
            myvar = setInterval(Marquee, 30);
        }
        demo.onmouseover = function () {
            clearInterval(myvar);
        }

        var orgPass = "";

        function doLogin() {
            CommonFunc.ajax.get("/getCode", function (data) {
                if (data.code == "0") {
                    orgPass = $("#loginPwd").val();
                    var key = data.data;
                    var encrypt = new JSEncrypt();
                    encrypt.setPublicKey(key['public']);
                    var encrypted = encrypt.encrypt($('#loginPwd').val());
                    $("#loginPwd").val(encrypted);
                    $("#code").val(key['public']);
                    var params = {'loginName': $("#loginName").val(), 'loginPwd': encrypted, 'verifyCode': $("#verifyCode").val(), 'code': key['public']};
                    CommonFunc.ajax.post("/login", params, function (response) {
                        if (response.code == '0') {
                            layer.msg('登录成功', {time: 1000, icon: 6}, function () {
                                location.reload();
                            });
                        } else if (response.code == 'E000002') {
                            layer.msg('验证码校验失败!', {time: 2000, icon: 5});
                            $("#loginPwd").val(orgPass);
                            $("#verifyCode").val("");
                        } else if (response.code == 'E010000') {
                            layer.msg('登录名或密码错误!', {time: 2000, icon: 5});
                            $("#loginPwd").val("");
                            $("#verifyCode").val("");
                        } else if (response.code == 'E010006') {
                            layer.msg('用户已被停用!', {time: 2000, icon: 5});
                            $("#loginPwd").val("");
                            $("#verifyCode").val("");
                        } else if (response.code == 'E010007') {
                            layer.msg('用户审核中!', {time: 2000, icon: 5});
                            $("#loginPwd").val("");
                            $("#verifyCode").val("");
                        }else if (response.code == 'E110006') {
                            //补全身份证消息
                            window.location.replace("/register/addIdCardInfo");
                        } else {
                            layer.msg('登录失败，请联系管理员!', {time: 2000, icon: 5});
                            $("#loginPwd").val("");
                            $("#verifyCode").val("");
                        }
                        $("#refreshImg").click();
                    });
                } else {
                    layer.msg('登录失败!', {time: 3000});
                }
            });

        }

        function getUnreadMsgCount(){
            $.post("/communication/unreadCount",function(result){
                if(result.code == "0"){
                    $("#unreadMsgCount").text("(未读留言:" + (result.data > 0 ? result.data : 0) + "条)");
                    $("#unreadMsgCount").click(function(){
                        window.location.href = "/communication/view";
                    });

                }
            });
        }

        $(document).ready(function () {
            //搜索类型
            $('#searchType').change(function () {
                var selected = $(this).children('option:selected').val();
                if (selected == 2) {
                    $('#search-input').attr("placeholder", "请输入要搜索的图片ID");
                    $('#search-input').attr("name", "pictureId");
                    $('#form').attr("action", "/search/searchPictures");
                } else {
                    $('#search-input').attr("placeholder", "请输入要搜索的关键词");
                    $('#search-input').attr("name", "keywords");
                    $('#form').attr("action", "/search/searchAlbums");
                }
            });
            $("#refreshImg").click(function () {
                var timestamp = (new Date()).valueOf();
                $("#codeImg").attr("src", "/verifyCode?t=" + timestamp);
                return false;
            });

            $("#reg").click(function () {
                window.location = '/register';
            });

            $('#retrieve').click(function () {
                window.location = '/retrieve/view';
            });
            getAnnouncement();

        });
    </script>
    
</div>

