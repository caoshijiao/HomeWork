                }
                var i;
                $.ajax({
                    type: "post",
                    url: '/communication/add',
                    data: {content: $("#content").val(), albumId: 1307133},
                    dataType: 'json',
                    cache: false,
                    beforeSend: function () {
                        i = ityzl_SHOW_LOAD_LAYER();
                    },
                    success: function (data) {
                        $('#content').val('');
                        initMessage();
                        ityzl_CLOSE_LOAD_LAYER(i);
                    }
                });
            });

            initMessage();

        });

        function initMessage() {
            if (null != null && null == 1153) {
                $.ajax({
                    type: "post",
                    url: '/communication/queryByAlbum',
                    data: {albumId: 1307133},
                    dataType: 'json',
                    cache: false,
                    success: function (data) {
                        var messageRecords = "";
                        if (null != data.data && data.data.length > 0) {
                            $.each(data.data, function (index, item) {
                                if (item.msgSource == 1) {
                                    //来源后台
                                    messageRecords += "<li class='messageSourceBack'><div style='float: right'><img src='/images/admin.jpg' style='width: 50px;height: 50px;margin-top: 0px;margin-bottom: 0px'></div><div class='alert alert-info' role='alert' style='float: right;width: 90%'>" + item.senderName + "（管理员）：";
                                } else {
                                    //来源前台
                                    messageRecords += "<li class='messageSourceFront'><div style='float: left'><img src='/images/user.jpg' style='width: 50px;height: 50px;margin-top: 0px;margin-bottom: 0px'></div><div class='alert alert-success' role='alert' style='float: left;width: 90%'>" + item.senderName + "（作者）：";
                                }
                                messageRecords += item.content + "<br>[" + timestampToTime(item.createTime) + "]</div></li>";
                            });
                        }
                        $("#messageContent").html(messageRecords);
                    }
                });
            }
        }

        function timestampToTime(timestamp) {
            var date = new Date(timestamp * 1000);//时间戳为10位需*1000，时间戳为13位的话不需乘1000
            Y = date.getFullYear() + '-';
            M = (date.getMonth() + 1 < 10 ? '0' + (date.getMonth() + 1) : date.getMonth() + 1) + '-';
            D = date.getDate() + ' ';
            h = date.getHours() + ':';
            m = (date.getMinutes() < 10 ? '0' + (date.getMinutes()) : date.getMinutes()) + ':';
            s = (date.getSeconds() < 10 ? '0' + (date.getSeconds()) : date.getSeconds());
            return Y + M + D + h + m + s;
        }


