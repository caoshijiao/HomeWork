	<script type="text/javascript">
						// JavaScript Document
			function addCookie(){//加入收藏夹
				if(document.all){
				       window.external.addFavorite('http://www.scopsr.gov.cn', '中国机构编制网');
				}
				else if(window.sidebar){
				      window.sidebar.addPanel('中国机构编制网','http://www.scopsr.gov.cn',"");
				}
			}

			function setHomepage(){//设置首页
			   if(document.all){
			      document.body.style.behavior='url(#default#homepage)';
			      document.body.setHomePage('http://www.scopsr.gov.cn');
			   }
			   else if(window.sidebar){
			        if(window.netscape){
			             try{
			                 netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
			             }
			             catch(e){
			                 alert("您的浏览器未启用[设为首页]功能，开启方法：先在地址栏内输入about:config,然后将项 signed.applets.codebase_principal_support 值该为true即可");
			             }
			        }
			        var prefs=Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefBranch);
			        prefs.setCharPref('browser.startup.homepage','http://www.scopsr.gov.cn');
			   }
			}
		</script>
