<script type="text/javascript">
$(document).ready(function() {
	var callback = function() {
		var $newsList = $(".cai");
	    if($newsRecommender.displayNews.length!=0){
		$newsList.empty();
	    }
		$.each($newsRecommender.displayNews, function(index, news) {
			var title = news.title;
			if (title.length > 19) {
				title = title.substr(0,18) +"...";
			}
			$newsList.append('<li><a target="_blank"></a></li>').find("a").last().attr({href: news.newsLink+'?form=rect', title: news.title }).text(title);
	  		//$newsList.append('<li><a href="'+ news.newsLink +'" target="_blank">'+ news.title +'</a></li>');
	  	});
	};
	
	var $newsRecommender = newsRecommender = new NewsRecommender();
	$newsRecommender.initialize(callback);

});
</script>
