<script type="text/javascript">
RMWSSO.getUserData(function(user){
	if(!user.userId){
		$("#txz_dlq").show();
		$("#txz_dlh").hide();
		//alert('δ��¼');
	}else{
		$("#txz_dlq").hide();
		$("#txz_dlh").show();
		var loginMsg = "<a href='http://sso.people.com.cn/login' target='_blank'>"+user.nickname+"</a>����ӭ��";
		$("#loginMsg").html(loginMsg);
		//alert('��ǰ��¼�û���' + user.nickname);
	}
});

function loginSuccess(){
	$("#txz_dlq").hide();
	$("#txz_dlh").show();
	RMWSSO.getUserData(function(user){
		if(user.userId){ 
		var loginMsg = "<a href='http://sso.people.com.cn/login' target='_blank'>"+user.nickname+"</a>����ӭ��";
		$("#loginMsg").html(loginMsg);
		}
	})
	$("#p_login").css({"display":"none"})
}

$("#logout").bind('click', function(event){
	RMWSSO.logout(function(){
		//alert('�˳��ɹ���')
		$("#txz_dlq").show();
		$("#txz_dlh").hide();
	});
})

</script>
