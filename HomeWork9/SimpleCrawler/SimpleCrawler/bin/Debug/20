				}else{
				sgwidth=parseInt(data[arr[i]].forumName.length*14+300);
				localHtml += '<li style="width:'+ sgwidth+'px">'+ hftime +'<a href="'+ data[arr[i]].threadUrl +'" target="_blank">���Ѹ�'+ data[arr[i]].forumName +'��ӳ�ġ�'+ data[arr[i]].domainName +'��������Ӧ</a></li>';
				}
				zlength=sgwidth+zlength;
			});
			localHtml +='</ul></div>'
			$("#ldlyb").html(localHtml);
			var num = 0;
			zlength=780-zlength;
			console.log(zlength);
			function goLeft() {
				if (num == zlength) {
					num = 0;
				}
				num -= 1;
				$(".scroll").css({
					left: num
				})
			}
			var timer = setInterval(goLeft,20);
			//������꾭��ʱ����ֹͣ
			$(".scroll").hover(function() {
				clearInterval(timer);
			},
			function() {
				timer = setInterval(goLeft,20);
			})
		},
		error: function(XMLHttpRequest, textStatus, errorThrown) {
			console.log(XMLHttpRequest.status);
			console.log(XMLHttpRequest.readyState);
			console.log(textStatus);
		}
	});
  }
ShuJu();	
})
</script>
