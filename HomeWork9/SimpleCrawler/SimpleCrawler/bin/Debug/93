<script type="text/javascript">
    $(document).ready(function() {
        $('.selet_options li').click(function() {
            $('.cls').val($(this).text());
            $('.selet_options').hide();
            $('#zhuye').html($(this).text());

        });
    });

    function listshow() {
        $('.selet_options').show();
    }

    function listhide() {
        $('.selet_options').hide();
    }
    $(".navigat .chanel li").each(function(i, item) {
        $(item).mouseover(function() {
            changeBg(i)
        })
    })
    $(".chanel-detail ul").each(function(i, item) {
        $(item).mouseover(function() {

            changeBg(i)
        })
    })
    $(".navigat").mouseleave(function() {
        hideChanel()
    });

    function hideChanel() {
        var xiangshu = $(".triangles").css('left');
        $(".chanel-detail").hide();
        $(".triangle").css('left', xiangshu);
    }

    function changeBg(i) {
        var leftP = 30;
        var isSelected = true;
        if (i == 0) {
            leftP = leftP;
        } else if (i == 1) {
            leftP = leftP + 70;
        } else {
            leftP = 30.5 + (71.3 * i)
        }
        if (isSelected) {
            $(".chanel-detail").show();
            $(".chanel-detail ul").eq(i).addClass("selected").siblings().removeClass("selected");
        }

        $(".triangle").css('left', leftP + "px");
    }
</script>     <!-- head 结束 -->
