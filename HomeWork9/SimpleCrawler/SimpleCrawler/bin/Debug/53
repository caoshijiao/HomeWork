<script src="/Javascripts/newindex/home.js?v=201810231536" type="text/javascript" charset="utf-8"></script>
<script type="text/javascript">
    function test(obj) {
        var words = document.getElementById("key").value;
        jQuery(document).ready(function() {
            jQuery("div[id='searchcityzicpode'] div[class='12']").remove();
        });
        $.ajax({
            url: 'https://www.youbian.com/?module=Home&action=ReturnAjax',
            type: "GET",
            data: {
                keyword: words
            },
            datanType: 'json',
            success: function(json) {
                if (typeof json == 'string') {
                    var oProduct;
                    var result = eval("(" + json + ")"); //转换为json对象
                    for (var i = 0, n = result.length; i < n; i++) {
                        oProduct = result[i];
                        var txt1 = '<div class=12>' + oProduct.name + '&nbsp;&nbsp;<em>' + oProduct.zipcode + '</em></div>'; // 以 HTML 创建新元素
                        //var txt2=$("<p></p>").text("Text.");   // 以 jQuery 创建新元素
                        //var txt3=document.createElement("p");  // 以 DOM 创建新元素
                        //txt3.innerHTML=oProduct.sname;
                        $("#stage").after(txt1);
                        $row = $(this);
                    }
                }
            }
        });
    }
    function youbianshow() {
        $("#searchcityzicpode").hide();
    }
    $('.peijia img').error(function() {
        $(this).attr('src', "/Images/newindex/341x228.jpg");
    });
</script>
